# int() str() bool()

login = "admin"
passwd = 123

login_ = input("Введите логин:")
passwd_ = int(input("Введите пароль:"))

if (login == login_):
    print("login_:", login_)
    if (passwd == passwd_):
        print("passwd_:", passwd_)
        print("Вы успешно вошли в систему!")
    else:
        print("Логин и пароль не совпадает!")
else:
    print("Логин и пароль не совпадает!")

# todo: Определить в коде переменные:
# 1. Целочисленного типа - + = (int - passwd)
# 2. Вещественного типа - - = float (дробные числа)
# 3. Логического типа - + = (bool - login == login_)
# 4. Строкового типа - + = (str - login)
# 5. Пустого типа - - = print() - ( "" / () / {} / [] )
# Вывести их типы (надо погуглить). -

print(type(login)) # вывел тип - строку (str)
print(type(passwd)) # вывел тип - целое число (int)
print(type(login == login_)) # вывел тип - логический (bool - true/false)
# print(type(("passwd_:", passwd_))) #tuple (кортежи)
print(type(print())) # для себя тип функции
print(type(int))
